@model BT_NHOM.Models.Quyen

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h4 style="color:red;"> Bạn có chắc chắn xóa thông tin này?</h4>
<table border="0" cellpadding="0" cellspacing="0" style="font-size:14px;margin-left:50px;">
    <tr>
        <td>
            <table cellpadding="2" cellspacing="10">
                <b>THÔNG TIN CHI TIẾT</b>

                <tr>
                    <td>Tên quyền:</td>
                    <td>@Html.DisplayFor(model => model.IDQuyen)</td>
                </tr>
                <tr>
                    <td>Mô tả:</td>
                    <td>@Html.DisplayFor(model => model.TenQuyen)</td>
                </tr>

            </table>
        </td>
    </tr>
</table>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-actions no-color" style="margin-left:50px;">
        <input type="submit" value="Xóa" class="btn btn-default" /> |
        @Html.ActionLink("Quay lại", "Index")
    </div>
}
